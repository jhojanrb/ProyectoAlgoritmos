@article{ref_dup2429,
  title = {Principles of problem solving}},
  author = {Wegner P.; Goldin D.}},
  year = {2006}},
  journal = {Communications of the ACM}},
  abstract = {It has been suggested that changed modes of human thought are needed to match changes in the paradigm of computational problem solving caused by the Internet and several other evolving forms of computing. The contrasting approaches of a priori rationalism and interactive empiricism in modeling new forms of computational technology that modify historical, philosophical, and mathematical forms of thoughts were explored. The restriction of problem solving to thinking and question answering excludes interactive forms of problem solving that depend on the behavior of the world rather than on a priori human beliefs. The ongoing support for rationalist over empiricist modes of thought suggests that human thinking is inherently more concerned with the rationality of human desires than with the empirical truth of human reasoning. Computation emphasizes open processes involving interaction among machines and users rather than the closed transformation of an input to an output.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-33746098538},
  shared_files = {Data/resultados_ACM.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup2430,
  title = {The aProS project: Strategic thinking & computational logic}},
  author = {Sieg W.}},
  year = {2007}},
  journal = {Logic Journal of the IGPL}},
  abstract = {The paper discusses tools for teaching logic used in Logic & Proofs, a web-based introduction to modern logic that has been taken by more than 1,300 students since the fall of 2003. The tools include a wide array of interactive learning environments or cognitive mini-tutors; most important among them is the Carnegie Proof Lab. The Proof Lab is a sophisticated interface for constructing natural deduction proofs and is central, as strategically guided discovery of proofs is the distinctive focus of the course. My discussion makes explicit the broader intellectual context, but also the pursuit of pedagogical goals and their experimental examination. The intellectual context includes i) the theoretical background for the proof search algorithm AProS and its use for a dynamic Proof Tutor, and ii) the programmatic expansion of the course to Computational Logic. (I recommend that the reader enter the virtual classroom of Logic & Proofs: the interactive components just cannot be properly reflected in a narrative. It is also very easy to download AProS and observe its ways of searching for proofs.). © The Author, 2007. Published by Oxford University Press.}},
