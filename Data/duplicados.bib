}

@article{ref_dup803,
  title = {Can GPT-4 Think Computationally about Digital Archival Practices?}},
  author = {Underwood W.; Gage J.}},
  year = {2023}},
  journal = {Proceedings - 2023 IEEE International Conference on Big Data, BigData 2023}},
