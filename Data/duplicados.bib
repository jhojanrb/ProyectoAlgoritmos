@article{ref_dup2149,
  title = {DISSECT: An experiment in infusing computational thinking in K-12 science curricula}},
  author = {Arraki K.; Blair K.; Burgert T.; Greenling J.; Haebe J.; Lee G.; Peel A.; Szczepanski V.; Pontelli E.; Hug S.}},
