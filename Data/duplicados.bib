  author = {Mensing K.; Mak J.; Bird M.; Billings J.}},
  year = {2013}},
  journal = {ICETA 2013 - 11th IEEE International Conference on Emerging eLearning Technologies and Applications, Proceedings}},
  abstract = {The United States (U.S.) is going through a preschool through 12th grade education transition to Common Core State Standards (CCSS), initially English Language Arts (ELA) and Mathematics (Math). Computational and model thinking can lead to a practice of computer coding where many of the necessary shifts in learning the ELA and Math standards can be incorporated. A new initiative by Paradise Valley Unified School District (PVUSD) to teach coding to K-8 students, and the requisite computational/model thinking through CCSS activities and lesson plans is provided. Three Gifted Education teachers (1st/2nd grades, 3rd/4th grades, 5th/6th grades) and the Information Technology Director articulate and showcase some explorations of teaching computer coding. The teachers and their students have been exposed to a variety of computational/model thinking activities and web portals/platforms (e.g., Codecademy, Scratch, App Inventor, Khan Academy, etc.), IOS/Android Apps, and web sites as listed at http://ccc.pvlearners.net. All students have access to tablets, Chromebooks, and TelePresence units to facilitate their creation and collaboration in coding. © 2013 IEEE.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-84896972424},
  shared_files = {Data/resultados_ieee.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup2262,
  title = {PSI: A computational architecture of cognition, motivation, and emotion}},
  author = {Dörner D.; Güss C.D.}},
  year = {2013}},
  journal = {Review of General Psychology}},
  abstract = {This article describes PSI theory, which is a formalized computational architecture of human psychological processes. In contrast to other existing theories, PSI theory not only models cognitive, but also motivational and emotional processes and their interactions. The article starts with a brief overview of the theory showing the connections between its different parts. We then discuss the theory's components in greater detail. Key constructs and processes are the five basic human needs, the satisfaction of needs using the cognitive system, including perception, schemas in memory, planning, and action. Furthermore, emotions are defined and the role of emotions in cognitive and motivational processes is elaborated, referring to a specific example. The neural basis of the PSI theory is also highlighted referring to the "quad structure," to specific brain areas, and to thinking as scanning in a neural network. Finally, some evidence for the validity of the theory is provided. © 2013 American Psychological Association.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-84887538826},
  shared_files = {Data/resultados_Sage.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup2263,
  title = {Inter-applicative game strategy design algorithm}},
  author = {Padwalkar O.; Deshpande K.}},
  year = {2013}},
  journal = {2013 International Conference on Electronics, Computers and Artificial Intelligence, ECAI 2013}},
  abstract = {Design of new strategies according to game models has been a general approach from the start. There are numerous games and each game has a different set of rules and different set of strategies for playing them. Though the design model of the game is similar to a set of particular games of the same domain, still we have to have a different strategic model for every individual game. The strategy design for these games also has to be different. Hence the paper proposes a new model for the design of game strategy by generalising the approach. Virtual generation of various strategies with the inter-application of basic strategies and complex strategies from various set of games into one another is the proposed algorithm. This gives a new approach entirely to the modelling of game strategies. It does so with the help of trained networks which are dynamically created and virtually maintained. This paper proposes a entire new approach in game modelling; more generalized and a more integrated approach. © 2013 IEEE.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-84890874443},
  shared_files = {Data/resultados_ieee.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup2264,
