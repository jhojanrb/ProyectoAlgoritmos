}

@article{ref_dup1255,
  title = {Fun and Engaging Pre-CS1 Programming Languages}},
  author = {Garcia D.D.; Rogers M.P.; Stefik A.}},
  year = {2021}},
  journal = {SIGCSE 2021 - Proceedings of the 52nd ACM Technical Symposium on Computer Science Education}},
  abstract = {The CSforALL movement to bring computational thinking to K-12 has been a boon for practitioners and language developers. This panel features three educators passionate about a particular lan-guage that has been successful with K-12 audiences. Each will demonstrate their language, describe what makes it unique, and share some of the fun and engaging projects students have created. © 2021 Owner/Author.}},
