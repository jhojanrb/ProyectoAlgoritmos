@article{ref_dup1719,
  title = {Exploring the role of visual programming activities in computational thinking}},
  author = {Lin C.-C.; Lin E.-T.; Tzeng H.-L.; Chao P.-Y.}},
  year = {2018}},
  journal = {Proceedings - 2018 1st International Cognitive Cities Conference, IC3 2018}},
  abstract = {This study explored the role of visual programming activities in computational thinking. A group of 158 freshmen majoring in information science and communication in Northern Taiwan were engaged in this study. A visual programming platform, computational thinking questionnaire and a programming examination were employed to collect the log data of programming activities, self-responses to computational thinking and the score of programming examination for later analyses. The results reveal that visual programming activities would be critical to the evolution of multiple computational thinking perspectives. © 2018 IEEE.}},
