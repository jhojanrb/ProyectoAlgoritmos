}

@article{ref_dup1995,
  title = {Parsimonious Risk Assessment and the Role of Transparent Diverse Models}},
  author = {McSharry P.}},
  year = {2017}},
  journal = {Risk Modeling for Hazards and Disasters}},
  abstract = {With the rise of big data and computational power, one could be forgiven for thinking that it is just a matter of time before most complex systems, whether physical or socioeconomical, are understood, modeled, and eventually predicted with great precision. Alongside the growing volumes of information collected about human behavior through transactions, communications, and web searches, the Internet of Things promises to supply digital information to help us monitor machines and improve our everyday interactions with new devices. The temptation to construct evermore complex models is widespread, driven by the appearance of increased sophistication and the hope of improving predictability. The philosophical principle known as Occam's razor, however, warns against complexity and favors parsimonious models. When attempting to quantify risk, this guiding principle is as relevant today as it was when introduced in the 14th century. Transparency and ease of use are also important characteristics of models that will appeal to practitioners. Rather than seeking the holy grail of the illusive single optimal model, an ensemble of diverse parsimonious models may offer the best chance of accurately quantifying risk. © 2018 Elsevier Inc. All rights reserved.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-85140504847},
  shared_files = {Data/resultados_Scopus.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup1996,
  title = {Wireless sensor network performance research for LEACH based on multi-agent simulation}},
  author = {Liao C.; Zhu K.; Tang J.; Zhang S.}},
  year = {2017}},
  journal = {Proceedings - 2016 International Conference on Agents, ICA 2016}},
  abstract = {Due to the limited data transmission capability, most of the sensor nodes in the wireless sensor networks(WSN) cannot send data to the base station directly. In most situations the data transfer has to be done by the means of multi-hop. Low Energy Adaptive Clustering Hierarchy(LEACH) is one of the most widely used multi-hop strategies. However, as there are many random factors interrelated with each other in the networks, it's very difficult to evaluate the network performance for LEACH, which has become a hotpot of current research. Since lifetime is the most important network performance indicator, this paper collects the network operation parameters including working period through multi-agent simulation and computational experiments, and evaluates the network performance in different situations based on the collected data. The results would provide practical advices for optimizing the network performance and come up with a new thinking of adopting multi-agent simulation in WSN research.) © 2016 IEEE.}},
